{
  "mcpServers": {
    "cloudflare-bindings": {
      "command": "npx",
      "args": ["mcp-remote", "https://bindings.mcp.cloudflare.com/mcp"]
    },
    "cloudflare-docs": {
      "command": "npx",
      "args": ["mcp-remote", "https://docs.mcp.cloudflare.com/mcp"]
    },
    "github": {
      "command": "npx",
      "args": ["mcp-remote", "https://api.githubcopilot.com/mcp/"]
    }
  },
  "enableAllProjectMcpServers": true
}
